{
  "name": "snyk-go-parser",
  "description": "Generate a dep tree given a collection of manifests",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "lint:fix": "tslint -p tsconfig.json --fix",
    "build": "tsc",
    "build-watch": "tsc -w",
    "prepare": "npm run build",
    "format:check": "prettier --check {lib,tests,scripts}/**/*.{js,ts}",
    "format": "prettier --write {lib,tests,scripts}/**/*.{js,ts}",
    "lint": "npm run format:check && npm run format:eslint",
    "format:eslint": "eslint --cache . --ext .js,.ts"
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/snyk/snyk-go-parser.git"
  },
  "author": "snyk.io",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8"
  },
  "files": [
    "bin",
    "dist"
  ],
  "homepage": "https://github.com/snyk/snyk-go-parser#readme",
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "@snyk/dep-graph": "^2.5.0",
    "event-loop-spinner": "^2.2.0",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "prettier": "^2.8.2",
    "eslint-config-prettier": "^8.6.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.4",
    "ts-node": "10.9.1",
    "tslint": "5.20.1",
    "typescript": "^4.9.4"
  }
}
